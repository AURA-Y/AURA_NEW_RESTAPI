{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      {
        "include": "calendar/google-service-account.json",
        "outDir": "dist/src"
      },
      {
        "include": "../generated/**/*",
        "outDir": "dist/generated",
        "watchAssets": true
      }
    ]
  }
}
